name = "nav"
main = "src/index.js"
compatibility_date = "2025-12-17"

# D1 Database binding
[[d1_databases]]
binding = "DB"
database_name = "nav_db"
database_id = "YOUR_DATABASE_ID_HERE"

# Environment variables
[vars]
JWT_SECRET = "YOUR_JWT_SECRET_HERE"

# Static assets
[assets]
directory = "./public"
